                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_264pxv" android:layout_width="@dimen/_429pxh"/>
                    <View android:background="@color/blue_gray_100"
                        android:id="@+id/lineDivisinFive"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_306pxv" android:layout_width="@dimen/_428pxh"/>
                    <View android:background="@color/blue_gray_100"
