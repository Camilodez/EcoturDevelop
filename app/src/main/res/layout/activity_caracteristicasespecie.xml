                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_291pxv"
                        android:layout_marginTop="@dimen/_307pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/_5pxv"
                        android:paddingEnd="@dimen/_35pxh"
                        android:paddingStart="@dimen/_35pxh"
                        android:paddingTop="@dimen/_5pxv" style="@style/groupStylewhite_A700">
