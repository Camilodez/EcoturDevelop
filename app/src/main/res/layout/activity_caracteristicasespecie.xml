<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="caracteristicasespecieVM" type="com.mariajosesapplication.app.modules.caracteristicasespecie.data.viewmodel.CaracteristicasespecieVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumncMaraOne"
