<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="photoSiONoVM" type="com.mariajosesapplication.app.modules.photosiono.data.viewmodel.PhotoSiONoVM"/>
    </data>
    <FrameLayout android:id="@+id/framePhotoSiONo"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageFondoOne"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_fondo}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_fondo"/>
        <LinearLayout android:id="@+id/linearRowcheckmark"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/_54pxv"
            android:paddingEnd="@dimen/_111pxh"
            android:paddingStart="@dimen/_111pxh" android:paddingTop="@dimen/_54pxv">
            <ImageView android:id="@+id/imageCheckmarkOne"
                android:layout_height="@dimen/_55pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_768pxv"
                android:layout_width="@dimen/_55pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_checkmark}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
            <ImageButton android:id="@+id/btnCancelOne"
                android:layout_height="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_90pxh"
                android:layout_marginTop="@dimen/_768pxv"
                android:layout_width="@dimen/_56pxh"
                android:scaleType="fitCenter"
                android:src="@drawable/img_cancel" tools:ignore="ContentDescription"/>
        </LinearLayout>
    </FrameLayout>
</layout>
