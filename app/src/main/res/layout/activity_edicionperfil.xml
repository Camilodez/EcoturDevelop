<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="edicionperfilVM" type="com.mariajosesapplication.app.modules.edicionperfil.data.viewmodel.EdicionperfilVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumncMaraOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearEdicinde"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" android:paddingTop="@dimen/_64pxv">
            <FrameLayout android:id="@+id/frameStackcmaraOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_190pxv"
                android:layout_width="@dimen/_190pxh"
                android:paddingHorizontal="@dimen/_44pxh"
                android:paddingVertical="@dimen/_44pxv" style="@style/groupStylewhite_2">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:srcCompat="@tools:sample/avatars" />
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUsuario"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{edicionperfilVM.edicionperfilModel.txtUsuario}"
                style="@style/txtOpenSansromanregular15" tools:text="@string/lbl_usuario"/>
            <EditText android:ellipsize="end"
                android:hint="@string/msg_correoelectr_nico_example_com"
                android:id="@+id/etEmailOne" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:paddingEnd="@dimen/_1pxh"
                android:paddingStart="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@={edicionperfilVM.edicionperfilModel.etEmailOneValue}"
                android:textColorHint="@color/blue_gray_100"
                style="@style/etOpenSansromanregular15" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCorreoelectr"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{edicionperfilVM.edicionperfilModel.txtCorreoelectr}"
                style="@style/txtOpenSansromanregular15" tools:text="@string/msg_correo_electr_nico"/>
            <EditText android:ellipsize="end"
                android:hint="@string/msg_correoelectr_nico_example_com"
                android:id="@+id/etEmailThree" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingEnd="@dimen/_1pxh"
                android:paddingStart="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@={edicionperfilVM.edicionperfilModel.etEmailThreeValue}"
                android:textColorHint="@color/blue_gray_100"
                style="@style/etOpenSansromanregular15" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtContrasea"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{edicionperfilVM.edicionperfilModel.txtContrasea}"
                style="@style/txtOpenSansromanregular15" tools:text="@string/lbl_contrase_a"/>
            <EditText android:ellipsize="end" android:hint="@string/lbl"
                android:id="@+id/etLanguage" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@={edicionperfilVM.edicionperfilModel.etLanguageValue}"
                android:textColorHint="@color/blue_gray_100"
                style="@style/etOpenSansromanregular40" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtVuelvea"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{edicionperfilVM.edicionperfilModel.txtVuelvea}"
                style="@style/txtOpenSansromanregular15" tools:text="@string/msg_vuelve_a_colocar"/>
            <EditText android:ellipsize="end" android:hint="@string/lbl"
                android:id="@+id/etLanguage1" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@={edicionperfilVM.edicionperfilModel.etLanguage1Value}"
                android:textColorHint="@color/blue_gray_100"
                style="@style/etOpenSansromanregular40" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnModificar"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_64pxv"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginTop="@dimen/_51pxv"
                android:layout_width="@dimen/_290pxh"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@string/msg_modificar_perfil"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/msg_modificar_perfil"/>
        </LinearLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackmenOne"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60pxv">
                <ImageView android:id="@+id/imageMenOne"
                    android:layout_width="@dimen/_430pxh"
                    android:layout_height="@dimen/_60pxh"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_footer"
                    tools:ignore="ContentDescription" />

                <LinearLayout android:id="@+id/linearRowbinoculars"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_horizontal">

                    <ImageView android:id="@+id/imageBinocularsOne"
                        android:layout_width="@dimen/_30pxh"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_binoculars"
                        android:layout_marginEnd="8dp"
                        tools:ignore="ContentDescription" />

                    <ImageView android:id="@+id/imageOphthalmology"
                        android:layout_width="@dimen/_30pxh"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_gravity="center"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_ophthalmology"
                        tools:ignore="ContentDescription" />

                    <ImageView android:id="@+id/imageHomeOne"
                        android:layout_width="@dimen/_30pxh"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_gravity="center"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" />

                    <FrameLayout android:id="@+id/frameStackpersonOne"
                        android:layout_width="@dimen/_42pxh"
                        android:layout_height="@dimen/_60pxv"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_6pxh"
                        android:paddingStart="@dimen/_6pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:layout_marginEnd="8dp"
                        style="@style/groupStyleblue_gray_100">
                        <ImageView android:id="@+id/imagePersonOne"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_person"
                            tools:ignore="ContentDescription" />
                    </FrameLayout>

                    <ImageView android:id="@+id/imageOpenpaneOne"
                        android:layout_width="@dimen/_30pxh"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_gravity="center"
                        android:layout_marginStart="8dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_open_pane"
                        tools:ignore="ContentDescription" />
                </LinearLayout>
            </FrameLayout>

        </FrameLayout>
    </LinearLayout>
</layout>
