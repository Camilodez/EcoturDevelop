<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="edicionfotografiaVM" type="com.mariajosesapplication.app.modules.edicionfotografia.data.viewmodel.EdicionfotografiaVM"/>
    </data>
    <LinearLayout android:id="@+id/linearEdiciNFotografA"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_24pxv"
        android:paddingTop="@dimen/_24pxv" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearRowbackOne"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_17pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_15pxv" style="@style/groupStylewhite_1">
            <ImageView android:id="@+id/imageBackOne"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_35pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_35pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_back"
                tools:ignore="ContentDescription" tools:src="@drawable/img_back"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNombreOne"
                android:layout_gravity="bottom"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_130pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{edicionfotografiaVM.edicionfotografiaModel.txtNombreOne}"
                style="@style/txtOpenSansromanextrabold25" tools:text="@string/lbl_post"/>
        </LinearLayout>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearRowsunOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageSunOne"
                android:layout_height="@dimen/_25pxh"
                android:layout_marginBottom="@dimen/_138pxv"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_25pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_sun"
                tools:ignore="ContentDescription" tools:src="@drawable/img_sun"/>
            <ImageView android:id="@+id/imageImage"
                android:layout_height="@dimen/_177pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_width="@dimen/_121pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_rectangle_22}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_22"/>
        </LinearLayout>
        <View android:background="@color/blue_gray_100"
            android:id="@+id/lineLinetwoOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_61pxv" android:layout_width="@dimen/_430pxh"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowaddressOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <FrameLayout android:id="@+id/frameStackaddress"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_35pxv" android:layout_width="@dimen/_35pxh">
                <ImageView android:id="@+id/imageAddressOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_address"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_address"/>
                <View android:background="@drawable/img_address_35x35"
                    android:id="@+id/viewAddressThree"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_35pxh" android:layout_width="@dimen/_35pxh"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLocacin"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{edicionfotografiaVM.edicionfotografiaModel.txtLocacin}"
                style="@style/txtOpenSansromanbold15" tools:text="@string/lbl_locaci_n"/>
            <ImageView android:id="@+id/imageForwardOne"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_230pxh"
                android:layout_width="@dimen/_35pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_forward"
                tools:ignore="ContentDescription" tools:src="@drawable/img_forward"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowatsignOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageAtsignOne"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_35pxh"
                android:layout_width="@dimen/_35pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_at_sign"
                tools:ignore="ContentDescription" tools:src="@drawable/img_at_sign"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMencin"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{edicionfotografiaVM.edicionfotografiaModel.txtMencin}"
                style="@style/txtOpenSansromanbold15" tools:text="@string/lbl_menci_n"/>
            <ImageView android:id="@+id/imageForwardThree"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_230pxh"
                android:layout_width="@dimen/_35pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_forward"
                tools:ignore="ContentDescription" tools:src="@drawable/img_forward"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowforwardFive"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowearthplanet"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEarthplanetOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_earth_planet"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_earth_planet"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtTodospueden"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{edicionfotografiaVM.edicionfotografiaModel.txtTodospueden}"
                    style="@style/txtOpenSansromanbold15" tools:text="@string/msg_todos_pueden_ver"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageForwardFive"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_width="@dimen/_35pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_forward"
                tools:ignore="ContentDescription" tools:src="@drawable/img_forward"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowforward"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowsharerounded"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSharerounded"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_share_rounded"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_share_rounded"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtCompartira"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{edicionfotografiaVM.edicionfotografiaModel.txtCompartira}"
                    style="@style/txtOpenSansromanbold15" tools:text="@string/msg_compartir_a_otra"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageForwardSeven"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_83pxh"
                android:layout_width="@dimen/_35pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_forward"
                tools:ignore="ContentDescription" tools:src="@drawable/img_forward"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnPublicar"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_64pxv"
            android:layout_marginBottom="@dimen/_5pxv"
            android:layout_marginTop="@dimen/_91pxv"
            android:layout_width="@dimen/_290pxh"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@string/lbl_publicar"
            android:textAllCaps="false" style="@style/btnSolidRounded" tools:text="@string/lbl_publicar"/>
    </LinearLayout>
</layout>
