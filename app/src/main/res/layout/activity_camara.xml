<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="camaraVM" type="com.mariajosesapplication.app.modules.camara.data.viewmodel.CamaraVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCamara"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageFondoOne"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_fondo}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_fondo"/>
        <LinearLayout android:id="@+id/linearColumnflashoff"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_16pxv" android:paddingTop="@dimen/_16pxv">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowflashoffOne"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="@dimen/_15pxh" android:paddingStart="@dimen/_15pxh">
                    <ImageView android:id="@+id/imageImagesevenOne"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_image_7"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_7"/>
                    <ImageView android:id="@+id/imageFlashoffOne"
                        android:layout_height="@dimen/_35pxh"
                        android:layout_marginStart="@dimen/_334pxh"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_flash_off"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_flash_off"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <View android:id="@+id/viewView"
                android:layout_height="@dimen/_771pxh" android:layout_width="@dimen/_403pxh"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowsquareborder"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_39pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_55pxh"
                    android:layout_width="@dimen/_55pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle_21}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_21"/>
                <ImageView android:id="@+id/imageSquareborder"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_55pxh"
                    android:layout_marginStart="@dimen/_117pxh"
                    android:layout_width="@dimen/_55pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_square_border}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_square_border"/>
                <ImageView android:id="@+id/imageSwitchcamera"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_55pxh"
                    android:layout_marginStart="@dimen/_121pxh"
                    android:layout_width="@dimen/_55pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_switch_camera}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_switch_camera"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
