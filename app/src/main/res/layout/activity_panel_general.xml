                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_width="@dimen/_43pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_plant"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_plant"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFlora"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{panelGeneralVM.panelGeneralModel.txtFlora}"
                        style="@style/txtOpenSansromanbold12" tools:text="@string/lbl_flora2"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageBinoculares"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_9pxv"
