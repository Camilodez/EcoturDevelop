                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_3pxh"
                    android:paddingStart="@dimen/_3pxh"
                    android:paddingTop="@dimen/_7pxv" style="@style/groupStylegray_50cornerRadius">
                    <ImageView android:id="@+id/imageSparrowhawkOne"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_width="@dimen/_43pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_sparrowhawk"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_sparrowhawk"/>
                    <TextView android:ellipsize="end"
