                            android:id="@+id/txtDate"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{panelGeneralVM.panelGeneralModel.txtDate}"
                            style="@style/txtOpenSansitalicregular9" tools:text="@string/lbl_2023_05_22"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNombrefive"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{panelGeneralVM.panelGeneralModel.txtNombrefive}"
                            style="@style/txtOpenSansitalicregular9" tools:text="@string/lbl_tiram_chic"/>
