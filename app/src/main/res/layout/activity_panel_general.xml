                            android:layout_height="@dimen/_125pxh"
                            android:layout_width="@dimen/_224pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_5pxh}"
                            imageRes="@{@drawable/img_image_3}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image_3"/>
                        <ImageView android:id="@+id/imageImagefourOne"
                            android:layout_height="@dimen/_91pxh"
                            android:layout_width="@dimen/_223pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_5pxh}"
                            imageRes="@{@drawable/img_image_4}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image_4"/>
